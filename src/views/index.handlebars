<div class="product-container">
    <table class="tab">
        <thead>
            <tr>
                <td>Id</td>
                <td>Título</td>
                <td>Descripción</td>
                <td>Precio</td>
                <td>Categoría</td>
                <td>Código</td>
                <td>Stock</td>
            </tr>
        </thead>
        {{#each productosListados}}
        <tbody>
                    <td>{{this.id}}</td>
                    <td>{{this.title}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.category}}</td>
                    <td>{{this.code}}</td>
                    <td>{{this.stock}}</td>
        </tbody>
        {{/each}}
    </table>
</div>
